package core.db;

import java.sql.ResultSet;
import java.util.ArrayList;
import java.util.List;

public class Utils {

	
	public static Boolean isNumeric( String data ){
		try{
			Long.parseLong(data);			
		} catch (Exception e) {
			return false;
		}
		return true;
	}
	
	
